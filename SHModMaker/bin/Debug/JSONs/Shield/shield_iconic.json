{
   "mixins": "stonehearth:mixins:item_properties",
   "type": "entity",
   "components": {
      "item" : {
         "category" : "gear"   
      },
      "unit_info" : {
         "name": "~name~",
         "description": "~desc~",
         "icon" : "file(~iname~.png)"
      },
      "model_variants": {
         "default": {
            "models": [
               "file(~iname~_iconic.qb)"
            ]
         }
      },
      "mob" : {
         "model_origin" : { "x": -0.05, "y": 0, "z": 0.05 }
      },
      "stonehearth:material" : {
         "tags" : "~tags~"
      }
   }
}